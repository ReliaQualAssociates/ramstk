various widgets.  These values can be changed by the user to personalize their
experience. -->

<root>
    <tree name="MECHANISMS">
        <column>
            <defaulttitle>Mechanism ID</defaulttitle>
            <usertitle>Mechanism ID</usertitle>
            <datatype>gint</datatype>
            <position>0</position>
            <editable>0</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Failure Mechanism</defaulttitle>
            <usertitle>Failure Mechanism</usertitle>
            <datatype>gchararray</datatype>
            <position>1</position>
            <editable>0</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Root Cause</defaulttitle>
            <usertitle>Root Cause</usertitle>
            <datatype>gchararray</datatype>
            <position>2</position>
            <editable>0</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>RPN Occurrence</defaulttitle>
            <usertitle>RPN Occurrence</usertitle>
            <datatype>gint</datatype>
            <position>3</position>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>New RPN Occurrence</defaulttitle>
            <usertitle>New RPN Occurrence</usertitle>
            <datatype>gint</datatype>
            <position>4</position>
            <editable>1</editable>
            <visible>1</visible>
        </column>
    </tree>
</root>
