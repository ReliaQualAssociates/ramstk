<!-- This file contains information used by the RTK application to draw
various widgets.  These values can be changed by the user to personalize their
experience. -->

<root>
    <tree name="Stakeholder">
        <column>
            <defaulttitle>Input ID</defaulttitle>
            <usertitle>Input ID</usertitle>
            <datatype>gint</datatype>
            <position>0</position>
            <widget>text</widget>
            <editable>0</editable>
            <visible>0</visible>
        </column>
        <column>
            <defaulttitle>Stakeholder</defaulttitle>
            <usertitle>Stakeholder</usertitle>
            <datatype>gchararray</datatype>
            <position>1</position>
            <widget>combo</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Description</defaulttitle>
            <usertitle>Description</usertitle>
            <datatype>gchararray</datatype>
            <position>2</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Affinity Group</defaulttitle>
            <usertitle>Affinity Group</usertitle>
            <datatype>gchararray</datatype>
            <position>3</position>
            <widget>combo</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Priority</defaulttitle>
            <usertitle>Priority</usertitle>
            <datatype>gint</datatype>
            <position>4</position>
            <widget>spin</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Satisfaction
with Existing
Product</defaulttitle>
            <usertitle>Satisfaction
with Existing
Product</usertitle>
            <datatype>gint</datatype>
            <position>5</position>
            <widget>spin</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Planned
Satisfaction
Rating</defaulttitle>
            <usertitle>Planned
Satisfaction
Rating</usertitle>
            <datatype>gint</datatype>
            <position>6</position>
            <widget>spin</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Improvement
Factor</defaulttitle>
            <usertitle>Improvement
Factor</usertitle>
            <datatype>gfloat</datatype>
            <position>7</position>
            <widget>text</widget>
            <editable>0</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Overall
Weighting</defaulttitle>
            <usertitle>Overall
Weighting</usertitle>
            <datatype>gfloat</datatype>
            <position>8</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Requirement</defaulttitle>
            <usertitle>Requirement</usertitle>
            <datatype>gchararray</datatype>
            <position>9</position>
            <widget>combo</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>User Float 1</defaulttitle>
            <usertitle>User Float 1</usertitle>
            <datatype>gfloat</datatype>
            <position>10</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>0</visible>
        </column>
        <column>
            <defaulttitle>User Float 2</defaulttitle>
            <usertitle>User Float 2</usertitle>
            <datatype>gfloat</datatype>
            <position>11</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>0</visible>
        </column>
        <column>
            <defaulttitle>User Float 3</defaulttitle>
            <usertitle>User Float 3</usertitle>
            <datatype>gfloat</datatype>
            <position>12</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>0</visible>
        </column>
        <column>
            <defaulttitle>User Float 4</defaulttitle>
            <usertitle>User Float 4</usertitle>
            <datatype>gfloat</datatype>
            <position>13</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>0</visible>
        </column>
        <column>
            <defaulttitle>User Float 5</defaulttitle>
            <usertitle>User Float 5</usertitle>
            <datatype>gfloat</datatype>
            <position>14</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>0</visible>
        </column>
    </tree>
</root>
