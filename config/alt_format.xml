<!-- This file contains information used by the RTK application to draw
various widgets.  These values can be changed by the user to personalize their
experience. -->

<root>
    <tree name="ALT">
        <column>
            <defaulttitle>Mode ID</defaulttitle>
            <usertitle>Mode ID</usertitle>
            <datatype>gint</datatype>
            <position>0</position>
            <widget>text</widget>
            <editable>0</editable>
            <visible>0</visible>
        </column>
        <column>
            <defaulttitle>Mechanism ID</defaulttitle>
            <usertitle>Mechanism ID</usertitle>
            <datatype>gint</datatype>
            <position>1</position>
            <widget>text</widget>
            <editable>0</editable>
            <visible>0</visible>
        </column>
        <column>
            <defaulttitle>Mechanism Description</defaulttitle>
            <usertitle>Mechanism Description</usertitle>
            <datatype>gchararray</datatype>
            <position>2</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Damaging Conditions</defaulttitle>
            <usertitle>Damaging Conditions</usertitle>
            <datatype>gchararray</datatype>
            <position>3</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Damage Model</defaulttitle>
            <usertitle>Damage Model</usertitle>
            <datatype>gchararray</datatype>
            <position>4</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Primary Measureable Parameter</defaulttitle>
            <usertitle>Primary Measureable Parameter</usertitle>
            <datatype>gchararray</datatype>
            <position>5</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Secondary Measureable Parameter</defaulttitle>
            <usertitle>Secondary Measureable Parameter</usertitle>
            <datatype>gchararray</datatype>
            <position>6</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Tertiary Measureable Parameter</defaulttitle>
            <usertitle>Tertiary Measureable Parameter</usertitle>
            <datatype>gchararray</datatype>
            <position>7</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Mean Load History</defaulttitle>
            <usertitle>Mean Load History</usertitle>
            <datatype>gchararray</datatype>
            <position>8</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Required Boundary Conditions</defaulttitle>
            <usertitle>Required Boundary Conditions</usertitle>
            <datatype>gchararray</datatype>
            <position>9</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
    </tree>
</root>
