<!-- This file contains information used by the RTK application to draw
various widgets.  These values can be changed by the user to personalize their
experience. -->

<root>
    <tree name="RGIncidents">
        <column>
            <defaulttitle>Test ID</defaulttitle>
            <usertitle>Test ID</usertitle>
            <datatype>gint</datatype>
            <position>0</position>
            <widget>text</widget>
            <editable>0</editable>
            <visible>0</visible>
        </column>
        <column>
            <defaulttitle>Test Code</defaulttitle>
            <usertitle>Test Code</usertitle>
            <datatype>gchararray</datatype>
            <position>1</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Test Type</defaulttitle>
            <usertitle>Test Type</usertitle>
            <datatype>gint</datatype>
            <position>2</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Short Description</defaulttitle>
            <usertitle>Short Description</usertitle>
            <datatype>gchararray</datatype>
            <position>3</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Long Description</defaulttitle>
            <usertitle>Long Description</usertitle>
            <datatype>gchararray</datatype>
            <position>4</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Start Date</defaulttitle>
            <usertitle>Start Date</usertitle>
            <datatype>gchararray</datatype>
            <position>5</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>End Date</defaulttitle>
            <usertitle>End Date</usertitle>
            <datatype>gchararray</datatype>
            <position>6</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Total Test Hours</defaulttitle>
            <usertitle>Total Test Hours</usertitle>
            <datatype>gchararray</datatype>
            <position>7</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
    </tree>
</root>
