            <usertitle>Test Type</usertitle>
            <datatype>gint</datatype>
            <position>2</position>
            <widget>text</widget>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Short Description</defaulttitle>
            <usertitle>Short Description</usertitle>
            <datatype>gchararray</datatype>
            <position>3</position>
            <widget>text</widget>
