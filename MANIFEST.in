include *

exclude .bandit
exclude .bandit.baseline
exclude .coverage
exclude .gitattributes
exclude .gitcommittemplate.txt
exclude .gitignore
exclude .pre-commit-config.yaml
exclude .python-version
exclude cobertura.xml

graft src
graft data
graft docs
graft locale

prune docs/_build*
prune docs/theme*
prune docs/user*

recursive-exclude tests *.py *.sql

global-exclude Makefile
global-exclude *.log
global-exclude *.nojekyll
global-exclude *.rst
global-exclude Guardfile
global-exclude conf.py
