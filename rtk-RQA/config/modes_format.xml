<!-- This file contains information used by the RTK application to draw
various widgets.  These values can be changed by the user to personalize their
experience. -->

<root>
    <tree name="MODES">
        <column>
            <defaulttitle>FMEA ID</defaulttitle>
            <usertitle>FMEA ID</usertitle>
            <datatype>gint</datatype>
            <position>0</position>
            <editable>0</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Assembly ID</defaulttitle>
            <usertitle>Assembly ID</usertitle>
            <datatype>gint</datatype>
            <position>1</position>
            <editable>0</editable>
            <visible>0</visible>
        </column>
        <column>
            <defaulttitle>Mode ID</defaulttitle>
            <usertitle>Mode ID</usertitle>
            <datatype>gint</datatype>
            <position>2</position>
            <editable>0</editable>
            <visible>0</visible>
        </column>
        <column>
            <defaulttitle>Failure Mode</defaulttitle>
            <usertitle>Failure Mode</usertitle>
            <datatype>gchararray</datatype>
            <position>3</position>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Mode Ratio, Original</defaulttitle>
            <usertitle>Mode Ratio, Original</usertitle>
            <datatype>gfloat</datatype>
            <position>4</position>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Mode Ratio, Final</defaulttitle>
            <usertitle>Mode Ratio, Final</usertitle>
            <datatype>gfloat</datatype>
            <position>5</position>
            <editable>1</editable>
            <visible>1</visible>
        </column>
        <column>
            <defaulttitle>Mission Phase</defaulttitle>
            <usertitle>Mission Phase</usertitle>
            <datatype>gchararray</datatype>
            <position>6</position>
            <editable>1</editable>
            <visible>1</visible>
        </column>
    </tree>
</root>
